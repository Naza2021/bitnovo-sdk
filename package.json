{
  "name": "bitnovo-sdk",
  "description": "Bitnovo sdk, On-off-ramp integration",
  "version": "0.1.8",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit --noEmitOnError && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "build-v2": "microbundle-crl --no-compress --format modern,cjs",
    "build-v3": "ts-node-dev --transpile-only src/build.ts && vite build"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.1.7",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.31.1",
    "autoprefixer": "^10.4.14",
    "bitnovo-server-types": "workspace:*",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "happy-dom": "^9.18.3",
    "microbundle-crl": "^0.13.11",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "ui": "workspace:*",
    "vite": "^4.3.7",
    "vite-plugin-dts": "^2.3.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.1",
    "ts-node-dev": "^2.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/my-lib.umd.js",
  "browser": "./dist/my-lib.umd.js",
  "module": "./dist/my-lib.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/my-lib.es.js",
      "require": "./dist/my-lib.umd.js"
    }
  }
}
